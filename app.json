{
  "expo": {
    "name": "Tecno Inventory App",
    "description": "Inventory app for Tecno Android phones",
    "slug": "inventory-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6faa8a"
    },
    "notification": {
      "icon": "./assets/images/notification.png"
    },
    "updates": {
      "enabled": true
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "android": {
      "package": "com.tecno.inventory",
      "permissions": [
        "NOTIFICATIONS"
      ]
    }
  }
}
